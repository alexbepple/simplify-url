{
  "scripts": {
    "done": "run-p --print-label lint:fix test build",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "test": "mocha --opts mocha.opts",
    "build": "rollup --config"
  },
  "private": true,
  "dependencies": {
    "query-string": "^6.1.0",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "esm": "^3.0.48",
    "hamjest": "^3.1.0",
    "mocha": "^5.0.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.5",
    "randomstring": "^1.2.2",
    "rollup": "^0.60.1",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "xo": "^0.21.1"
  },
  "xo": {
    "space": true,
    "prettier": true,
    "overrides": [
      {
        "files": "src/*.test.js",
        "envs": [
          "mocha"
        ]
      }
    ]
  }
}
